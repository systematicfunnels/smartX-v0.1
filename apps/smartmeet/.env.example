# SmartMeet - Environment Configuration
# Copy this file to .env and configure for your environment

# App Configuration
NEXT_PUBLIC_APP_NAME="SmartMeet"
NEXT_PUBLIC_APP_DESCRIPTION="AI-Powered Meeting Intelligence Platform"
NEXT_PUBLIC_API_BASE_URL="http://localhost:3000"

# Port Configuration
PORT=3001

# Theme Configuration
NEXT_PUBLIC_PRIMARY_COLOR="#3b82f6"
NEXT_PUBLIC_SECONDARY_COLOR="#1e40af"

# Feature Flags
NEXT_PUBLIC_FEATURE_MEETING_TRANSCRIPTION=true
NEXT_PUBLIC_FEATURE_ANALYTICS_DASHBOARD=true
NEXT_PUBLIC_FEATURE_INTEGRATIONS=true
