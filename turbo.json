{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "start": {
      "cache": false,
      "persistent": true
    },
    "test": {
      "dependsOn": ["^build"]
    },
    "db:migrate": {
      "dependsOn": [],
      "outputs": []
    },
    "infra:up": {
      "cache": false,
      "outputs": []
    },
    "infra:down": {
      "cache": false,
      "outputs": []
    }
  }
}
